#include <iostream>
using namespace std;

int main()
{
for (int i = 1; i < 10; i++)
    {
        if (i == 2)
            continue; // continue means isko skip krdo
        cout << i << endl;
    }
    return 0;
}

---------------------------------------------------------------------------------------------------------------------------------------------------------------------------
*************************************Output*************************************************

1
3
4
5
6
7
8
9
