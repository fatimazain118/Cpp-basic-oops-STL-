#include<iostream>

using namespace std;

int main(){
    int num1, num2;

cout<<"Enter the value of num1: \n"; // '<<' is called Insertion operator
cin>> num1; // '>>' is called Extraction operator

cout<<"Enter the value of num2: \n"; // '<<' is called Insertion operator
cin>> num2; // '>>' is called Extraction operator

cout<<" The sum is "<< num1 + num2;

return 0;
}

// Compiler k reference me Insertion ya Extraction operator likhte hai
// a.exe ye software bngya hai jo hum kissi se bhi share kr skte hai
//g++ 64bit hmara compiler hai

---------------------------------------------------------------------------------------------------------------------------------------------------------------
*******************************Output*************************
Enter the value of num1: 
1
Enter the value of num2: 
3
 The sum is 4
